    </patternset>

    <patternset id="excluded.from.compilation.teclibrary">
        <patternset refid="excluded.from.module.teclibrary"/>
    </patternset>

    <path id="teclibrary.module.sourcepath">
        <dirset dir="${module.teclibrary.basedir}">
            <include name="src/main/java"/>
            <include name="src/main/resources"/>
        </dirset>
    </path>


    <target name="compile.module.teclibrary"
            depends="compile.module.teclibrary.production,compile.module.teclibrary.tests"
            description="Compile module TecLibrary"/>

    <target name="compile.module.teclibrary.production" depends="register.custom.compilers"
            description="Compile module TecLibrary; production classes">
        <mkdir dir="${teclibrary.output.dir}"/>
        <javac2 destdir="${teclibrary.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}"
                memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.teclibrary}/javac">
            <compilerarg line="${compiler.args.teclibrary}"/>
            <bootclasspath refid="teclibrary.module.bootclasspath"/>
            <classpath refid="teclibrary.module.production.classpath"/>
            <src refid="teclibrary.module.sourcepath"/>
            <patternset refid="excluded.from.compilation.teclibrary"/>
        </javac2>

        <copy todir="${teclibrary.output.dir}">
            <fileset dir="${module.teclibrary.basedir}/src/main/java">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
            <fileset dir="${module.teclibrary.basedir}/src/main/resources">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
        </copy>
    </target>

    <target name="compile.module.teclibrary.tests"
            depends="register.custom.compilers,compile.module.teclibrary.production"
            description="compile module TecLibrary; test classes" unless="skip.tests"/>

    <target name="clean.module.teclibrary" description="cleanup module">
        <delete dir="${teclibrary.output.dir}"/>
        <delete dir="${teclibrary.testoutput.dir}"/>
    </target>

    <target name="init" description="Build initialization">
        <!-- Perform any build initialization in this target -->
    </target>

    <target name="clean" depends="clean.module.teclibrary" description="cleanup all"/>

    <target name="build.modules" depends="init, clean, compile.module.teclibrary" description="build all modules"/>

    <target name="all" depends="build.modules" description="build all"/>
</project>